{
  "name": "@zoho/common-utils",
  "version": "1.33.0",
  "description": "This package consists of multiple javascript files which can be shared and reused",
  "main": "dist/common-utils.js",
  "scripts": {
    "test": "./node_modules/.bin/live-server --entry-file=./test/index.html --port=7002 --open=/?random=false",
    "prepublishOnly": "npm run bundle",
    "runKarma": "karma start karma.conf.js",
    "prepack": "npm run runKarma",
    "commit": "git-cz",
    "release": "standard-version",
    "amend": "git commit . --amend -m \"$(git log -1 --pretty=%B)\"",
    "dev": "webpack --watch --mode=development",
    "bundle": "webpack --mode=production && webpack --mode=development",
    "generateCases": "node ./test/cases/cssParser/index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://git.csez.zohocorpin.com/lyte.js/common-utils.git"
  },
  "author": {
    "name": "vignesh.prv",
    "email": "vignesh.prv@zohocorp.com"
  },
  "bundleDependencies": false,
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "deprecated": false,
  "devDependencies": {
    "karma-chrome-launcher": "3.1.0",
    "karma-jasmine": "4.0.1",
    "copy-webpack-plugin": "^7.0.0",
    "cz-conventional-changelog": "3.3.0",
    "husky": "6.0.0",
    "jasmine": "3.6.4",
    "karma": "6.3.2",
    "live-server": "^1.2.1",
    "standard-version": "^9.3.1",
    "webpack": "^5.11.1",
    "webpack-cli": "^4.3.1",
    "webpack-merge": "5.7.3"
  },
  "husky": {
    "hooks": {}
  },
  "keywords": [
    "common-utils"
  ],
  "license": "ISC"
}
