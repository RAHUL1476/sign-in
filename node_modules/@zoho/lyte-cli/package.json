{
 "name": "@zoho/lyte-cli",
 "version": "3.3.1",
 "description": "Lyte CLI - a command line tool, used to create, build and manage an application in Lyte.",
 "main": "lib/cli/cli.js",
 "bin": {
  "lyte": "./bin/lyte"
 },
