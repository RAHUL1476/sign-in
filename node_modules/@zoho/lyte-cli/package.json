  "jasmine": "3.7.0"
 },
 "dependencies": {
  "escodegen": "1.13.0",
  "espree": "6.2.1",
  "eslint": "6.8.0",
  "fuse.js": "3.2.0",
  "get-port": "3.1.0",
  "glob": "7.1.6",
  "less": "4.1.1",
  "@zoho/lyte-cli-utils": "1.0.5-7",
  "mime-types": "2.1.12",
  "puppeteer": "3.3.0",
  "sane": "4.1.0",
  "semver": "7.3.4",
  "unclosed-tag-finder": "0.0.18",
  "ws": "7.2.3"
 },
 "author": "Iswarya <iswarya.sl@zohocorp.com>",
 "license": "ISC",
 "directories": {
  "lib": "lib",
  "test": "test"
 },
